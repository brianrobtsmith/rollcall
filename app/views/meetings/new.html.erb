<h1>New Meeting</h1>

<%= form_tag(meetings_url, method: 'post') do %>

<div>
  <br>  <%= label_tag :held_on %><br/>
        <%= text_field_tag :held_on %>
  </div>

  <div>
    <br><%= label_tag :course_id %><br/>
    <select name="select">
      <option value="value1">Value 1</option>
      <option value="value2" selected>Value 2</option>
      <option value="value3">Value 3</option>
    </select>
  </div>
<p></p>
  <div>
    <%= submit_tag "Create Meeting" %>
  </div>
<% end %>
