<h1>New Meeting</h1>


<%= form_tag(meetings_url, method: 'post') do %>

<div>
  <br>  <%= label_tag :held_on %><br/>
        <%= text_field_tag :held_on %>
  </div>

  <div>
    <br><%= label_tag :course_id %><br/>
   <select name="course_id">
    <% @course.each do |course| %>
      <option value="<%= course.id %>"><%= course.title %></option>
    <% end %>
  </select>
  </div>
<p></p>
  <div>
    <%= submit_tag "Create Meeting" %>
  </div>
<% end %>
